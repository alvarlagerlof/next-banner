{
  "name": "next-banner",
  "version": "1.0.0-semantic-release",
  "description": "Generate Open Graph images for Next.js on build",
  "repository": {
    "type": "git",
    "url": "https://github.com/alvarlagerlof/next-banner"
  },
  "author": "Alvar LagerlÃ¶f",
  "license": "MIT",
  "keywords": [
    "react",
    "next",
    "next.js",
    "og",
    "og:image",
    "opengraph",
    "generate",
    "build"
  ],
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "files": [
    "dist",
    "dist",
    "src"
  ],
  "main": "dist/client/index.js",
  "types": "dist/client/index.d.ts",
  "bin": "dist/cli/index.js",
  "scripts": {
    "lint": "eslint ./src --ext .ts,tsx",
    "build:esbuild": "rimraf dist && node ./esbuild.js",
    "build:esbuild:watch": "rimraf dist && node ./esbuild.js watch",
    "build:ts": "tsc",
    "build:ts:watch": "tsc --watch",
    "build:watch": "concurrently \"yarn build:esbuild:watch\" \"yarn build:ts:watch\"",
    "build": "rimraf dist && yarn build:esbuild && yarn build:ts"
  },
  "peerDependencies": {
    "next": "^>=10",
    "react": ">=16"
  },
  "devDependencies": {
    "@types/node": "^16.4.8",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "commitizen": "^4.2.4",
    "concurrently": "^7.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.14.27",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "next": "11.1.3-canary.52",
    "prettier": "^2.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.2",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@corex/deepmerge": "^3.0.539",
    "chalk": "^5.0.1",
    "cli-progress": "^3.10.0",
    "get-port": "^6.1.2",
    "merge-anything": "^5.0.2",
    "object-merge-advanced": "^13.0.12",
    "ora": "^6.1.0",
    "puppeteer": "^13.5.1",
    "tasuku": "^1.0.2",
    "webpack": "^5.58.1",
    "webpack-shell-plugin-next": "^2.2.2"
  }
}
